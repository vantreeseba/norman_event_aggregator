
/*Waffles by Jesse Harlin*/


.waffles { margin-left: auto; margin-right: auto; max-width: 100%; min-width: 0px; width: 100%;overflow: hidden; padding:0}

.waffles .w-1, 
.waffles .w-2, 
.waffles .w-3, 
.waffles .w-4, 
.waffles .w-5, 
.waffles .w-6, 
.waffles .w-7, 
.waffles .w-8, 
.waffles .w-9, 
.waffles .w-10, 
.waffles .w-11, 
.waffles .w-12, 
.waffles .w-auto  { display: block; float: left; margin-left: 1%; margin-right: 1%; position: static; box-sizing: border-box; margin-top:0px; margin-bottom:0px; -moz-box-sizing: border-box;}

.waffles .w-alpha { margin-left: 0; }
.waffles .w-omega { margin-right: 0; }

.waffles  .w-alpha ~ .w-omega { clear: right; display: block; }

.waffles  .w-omega ~ .w-alpha { clear: left; display: block; }

.waffles  .w-alpha.w-omega { clear: both; display: block}

.waffles  .w-auto > .w-alpha { clear: none; clear: none;}
.waffles  .w-auto > .w-omega { clear: none; clear: none;}

/*Core Fluid Widths*/
.waffles .w-1 { width: 6.333%; }
.waffles .w-2 { width: 14.666%; }
.waffles .w-3 { width: 23%; }
.waffles .w-4 { width: 31.333%; }
.waffles .w-5 { width: 39.666%; }
.waffles .w-6 { width: 48%; }
.waffles .w-7 { width: 56.333%; }
.waffles .w-8 { width: 64.666%; }
.waffles .w-9 { width: 73%; }
.waffles .w-10 { width: 81.333%; }
.waffles .w-11 { width: 89.666%; }
.waffles .w-12 { width: 98%; }

/*Css3 Overrides for side margins*/
.waffles .w-1.w-alpha, .waffles .w-1.w-omega { width: 7.3333%; }
.waffles .w-2.w-alpha, .waffles .w-2.w-omega { width: 15.6666%; }
.waffles .w-3.w-omega, .waffles .w-3.w-alpha { width: 24%; }
.waffles .w-4.w-alpha, .waffles .w-4.w-omega { width: 32.3333%; }
.waffles .w-5.w-alpha, .waffles .w-5.w-omega { width: 40.6666%; }
.waffles .w-6.w-omega, .waffles .w-6.w-alpha { width: 49%; }
.waffles .w-7.w-alpha, .waffles .w-7.w-omega { width: 57.333%; }
.waffles .w-9.w-omega, .waffles .w-9.w-alpha { width: 74%; }
.waffles .w-8.w-alpha, .waffles .w-8.w-omega { width: 65.6%; }
.waffles .w-10.w-alpha, .waffles .w-10.w-omega { width: 82.3333%; }
.waffles .w-11.w-alpha, .waffles .w-11.w-omega { width: 90.6666%; }
.waffles .w-12.w-alpha.w-omega { width: 100%; }

